        {
            "name": "Markup: Raw",
            "scope": ["markup.inline.raw", "markup.raw.block.markdown"],
            "settings": {
                "foreground": "#56b6c2"
            }
        },
        {
            "name": "Markup: List Punctuation",
            "scope": "punctuation.definition.list_item.markdown",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffffff"
            }
